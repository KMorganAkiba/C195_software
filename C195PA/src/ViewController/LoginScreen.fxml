<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="310.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController.LoginScreen">
   <children>
      <Label fx:id="SystemTitleText" layoutX="14.0" layoutY="14.0" text="Appointment System Login" AnchorPane.leftAnchor="14.0">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="UsernameLabel" layoutX="116.0" layoutY="104.0" text="Username">
         <font>
            <Font name="Arial Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="PasswordLabel" layoutX="117.0" layoutY="174.0" text="Password">
         <font>
            <Font name="Arial Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="UsernameTxtbox" layoutX="76.0" layoutY="134.0" />
      <TextField fx:id="PasswordTxtbox" layoutX="76.0" layoutY="204.0" />
      <Button fx:id="SubmitButton" layoutX="122.0" layoutY="264.0" mnemonicParsing="false" onMouseClicked="#SubmitButton" text="Submit">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="LanguageDeclerationLabel" layoutX="135.0" layoutY="332.0" text="Label" AnchorPane.bottomAnchor="53.0" AnchorPane.leftAnchor="55.0">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
